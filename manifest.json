{
    "manifest_version": 2,

    "name": "YouTube no translate",
    "description": "This extension recovers original video titles and descriptions",
    "version": "0.0.1",

    "web_accessible_resources": ["background.js"],

    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["inject.js"]
        }
    ]
}
